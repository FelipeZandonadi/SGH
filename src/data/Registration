package data;
import java.util.List;

/**
 *
 * @author Acer
 */
public class Registration {
    private String id;
    private String birthday;
    private String healthCard;
    private String adress;
    private PlanType planType;
    private boolean isSmoker;
    private boolean isDrinker;
    private double collesterolLevel;
    private boolean hasDiabets;
    private boolean hasHearthDisiese;
    private List<String> surgeries;
    private List<String> allergies;

    public Registration() {
    }

    public Registration(String id, String birthday, String document, String healthCard, String adress, PlanType planType, boolean isSmoker, boolean isDrinker, double collesterolLevel, boolean hasDiabets, boolean hasHearthDisiese, List surgeries, List allergies) {
        this.id = id;
        this.birthday = birthday;
        this.healthCard = healthCard;
        this.adress = adress;
        this.planType = planType;
        this.isSmoker = isSmoker;
        this.isDrinker = isDrinker;
        this.collesterolLevel = collesterolLevel;
        this.hasDiabets = hasDiabets;
        this.hasHearthDisiese = hasHearthDisiese;
        this.surgeries = surgeries;
        this.allergies = allergies;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getBirthday() {
        return birthday;
    }

    public void setBirthday(String birthday) {
        this.birthday = birthday;
    }

    public String getHealthCard() {
        return healthCard;
    }

    public void setHealthCard(String healthCard) {
        this.healthCard = healthCard;
    }

    public String getAdress() {
        return adress;
    }

    public void setAdress(String adress) {
        this.adress = adress;
    }

    public PlanType getPlanType() {
        return planType;
    }

    public void setPlanType(PlanType planType) {
        this.planType = planType;
    }

    public boolean isIsSmoker() {
        return isSmoker;
    }

    public void setIsSmoker(boolean isSmoker) {
        this.isSmoker = isSmoker;
    }

    public boolean isIsDrinker() {
        return isDrinker;
    }

    public void setIsDrinker(boolean isDrinker) {
        this.isDrinker = isDrinker;
    }

    public double getCollesterolLevel() {
        return collesterolLevel;
    }

    public void setCollesterolLevel(double collesterolLevel) {
        this.collesterolLevel = collesterolLevel;
    }

    public boolean isHasDiabets() {
        return hasDiabets;
    }

    public void setHasDiabets(boolean hasDiabets) {
        this.hasDiabets = hasDiabets;
    }

    public boolean isHasHearthDisiese() {
        return hasHearthDisiese;
    }

    public void setHasHearthDisiese(boolean hasHearthDisiese) {
        this.hasHearthDisiese = hasHearthDisiese;
    }

    public List getSurgeries() {
        return surgeries;
    }

    public void setSurgeries(List surgeries) {
        this.surgeries = surgeries;
    }

    public List getAllergies() {
        return allergies;
    }

    public void setAllergies(List allergies) {
        this.allergies = allergies;
    }
    
    
}
